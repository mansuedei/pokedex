{
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "tabWidth": 2,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "tabWidth": 4,
        "parser": "angular",
        "htmlWhitespaceSensitivity": "strict"
      }
    }
  ]
}
